<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/feedback_item_layout">

    <TextView
        android:id="@+id/feedback_course_code"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="64dp"
        android:layout_marginStart="64dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Button"
        android:textSize="16sp"
        android:textStyle="normal|bold|italic"
        android:textColor="@android:color/background_dark"
        android:layout_alignBaseline="@+id/feedback_name"
        android:layout_alignBottom="@+id/feedback_name"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/feedback_name"
        android:layout_gravity="center"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="16sp"
        android:text="TextView"
        android:layout_marginTop="20dp"
        android:fontFamily="serif"
        android:textAllCaps="false"
        android:textColor="?android:attr/textColorLinkInverse"
        android:layout_marginLeft="67dp"
        android:layout_marginStart="67dp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/feedback_course_code"
        android:layout_toEndOf="@+id/feedback_course_code"
        android:textStyle="normal|bold|italic" />
</RelativeLayout>